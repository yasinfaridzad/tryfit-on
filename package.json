{
  "name": "tryon-ai",
  "version": "1.0.0",
  "description": "DeepAR shoe-try-on demo.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js --mode production && npm run copy-files",
    "copy-files": "mkdir -p dist && cpy 'public/**/*' dist && cpy 'node_modules/deepar/**/*' dist/deepar-resources",
    "dev": "webpack serve --mode development --open --port 8888"
  },
  "author": "Develop by yasinfaridzad",
  "license": "MIT",
  "devDependencies": {
    "cpy-cli": "^4.2.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "deepar": "^5.0.0"
  },
  "engines": {
    "node": "18.x"
  }
}
